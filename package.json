{
  "name": "e-learning",
  "version": "1.0.0",
  "description": "Social Network for RPA ",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "server1": "npm run server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:stage": "cross-env NODE_ENV=staging concurrently \"npm run server\" \"npm run client\"",
    "dev:local": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client "
  },
  "author": "Deshmukh and Suren",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.909.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^5.3.1",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.9",
    "mongoose-paginate-v2": "^1.3.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "uuid": "^8.3.1",
    "uuidv1": "^1.6.14"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "datatables.net": "^1.10.22",
    "jquery": "^3.5.1",
    "nodemon": "^2.0.5"
  }
}
